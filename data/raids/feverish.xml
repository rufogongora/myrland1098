<?xml version="1.0" encoding="UTF-8"?>
<raid>
	<!-- Message broadcast -->
	<!-- Broadcast types: warning, event, default, description, smallstatus, blueconsole, redconsole -->
	<announce delay="1000" type="event" message="A lo lejos de Andicas se ve una cantidad enorme de locos!" />
	<announce delay="25000" type="event" message="Los feverish citizen invaden Andicas!" />

	<!-- Area spawns - you can specify raid area or use range value -->
	<areaspawn delay="25000" fromx="1012" fromy="1029" fromz="7" tox="1064" toy="1066" toz="7">
		<monster name="Feverish Citizen" amount="35" /><!-- Specified amount -->
	</areaspawn>

	<areaspawn delay="30000" fromx="1012" fromy="1029" fromz="7" tox="1064" toy="1066" toz="7">
		<monster name="Feverish Citizen" minamount="35" maxamount="55" /><!-- Random amount in specified range -->
	</areaspawn>

</raid>
